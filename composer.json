{
    "name": "drinky78/silex-rest-service-providers",
    "description": "Package containing several services for marcojanssen/silex-rest skeleton",
    "keywords": ["silex", "rest", "api", "provider"],
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Janssen",
            "email": "dev@marcojanssen.net"
        },
        {
            "name": "Marco Radossi",
            "email": "marco.radossi@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "dflydev/doctrine-orm-service-provider": "1.0.*",
        "doctrine/orm": "~2.3",
        "saxulum/saxulum-doctrine-orm-manager-registry-provider": "2.0.*",
        "jms/serializer": "0.16.0",
        "jurjean/spray-persistence-bundle": "2.*",
        "silex/silex": "~1.2",
        "symfony/finder": "~2.3",
        "symfony/process": "~2.3",
        "symfony/property-access": "~2.3",
        "symfony/validator": "~2.3",
        "zendframework/zend-filter": "~2.3",
        "zendframework/zend-loader": "~2.3",
        "zircote/swagger-php": "0.8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*"
    },
    "suggest": {
        "marcojanssen/silex-rest": "This package is implemented by marcojanssen/silex-rest"
    },
    "autoload": {
        "psr-0": {
            "MRadossi\\": "src/"
        }
    }
}
